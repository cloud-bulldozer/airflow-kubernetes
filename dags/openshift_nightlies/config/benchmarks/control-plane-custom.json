{
    "benchmarks": [
        {
            "name": "scale-27",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "27",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "workload"
            }
        },
        {
            "name": "node-density",
            "workload": "kube-burner",
            "custom_cmd": "kube-burner ocp node-density --uuid=${UUID} --pods-per-node=245 --timeout=2h --pod-ready-threshold=5s --es-server=${ES_SERVER} --es-index=${ES_INDEX}"
        },
        {
            "name": "node-density-cni",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "custom_cmd": "kube-burner ocp node-density-cni --uuid=${UUID} --pods-per-node=245 --timeout=3h --es-server=${ES_SERVER} --es-index=${ES_INDEX}"
        },
        {
            "name": "node-density-heavy",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "custom_cmd": "kube-burner ocp node-density-heavy --uuid=${UUID} --pods-per-node=245 --timeout=3h --es-server=${ES_SERVER} --es-index=${ES_INDEX}"
        },
        {
            "name": "cluster-density",
            "workload": "kube-burner",
            "trigger_rule": "all_done",
            "custom_cmd": "kube-burner ocp cluster-density --uuid=${UUID} --iterations=500 --timeout=3h --es-server=${ES_SERVER} --es-index=${ES_INDEX}"
        }
    ]
}

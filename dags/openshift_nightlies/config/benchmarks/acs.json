{
    "benchmarks": [
        {
            "name": "baseline",
            "workload": "baseline-performance",
            "command": "./baseline_perf.sh"
        },
        {
            "name": "scale-25",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "25",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "workload"
            }
        },
        {
            "name": "cluster-density",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "cluster-density",
                "JOB_ITERATIONS": "500",
                "JOB_TIMEOUT": "18000",
                "STEP_SIZE": "30s",
                "QPS": "20",
                "BURST": "20",
                "METRICS_PROFILE": "metrics-profiles/acs-metrics.yaml",
                "LOG_LEVEL": "info",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }"
            }
        },
        {
            "name": "node-density",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "node-density",
                "PODS_PER_NODE": "245",
                "NODE_COUNT": "25",
                "JOB_TIMEOUT": "18000",
                "QPS": "20",
                "BURST": "20",
                "STEP_SIZE": "30s",
                "METRICS_PROFILE": "metrics-profiles/acs-metrics.yaml",
                "LOG_LEVEL": "info",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }"
            }
        },
        {
            "name": "scale-50",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "50",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "workload"
            }
        },
        {
            "name": "cluster-density-50",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "cluster-density",
                "JOB_ITERATIONS": "1000",
                "JOB_TIMEOUT": "18000",
                "STEP_SIZE": "30s",
                "QPS": "20",
                "BURST": "20",
                "METRICS_PROFILE": "metrics-profiles/acs-metrics.yaml",
                "LOG_LEVEL": "info",
                "CLEANUP_WHEN_FINISH": "true",
                "CLEANUP": "true",
                "MUST_GATHER_EACH_TASK": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }"
            }
        },
        {
            "name": "host-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "hostnet",
                "PAIRS": "1"
            }
        },
        {
            "name": "pod-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "pod2pod"
            }
        },
        {
            "name": "serviceip-network",
            "workload": "network-perf",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "pod2svc"
            }
        },
        {
            "name": "scale-down-25",
            "workload": "scale-perf",
            "command": "./run_scale_fromgit.sh",
            "env": {
                "SCALE": "25",
                "METADATA_COLLECTION": "true",
                "WORKLOAD_NODE_ROLE": "workload"
            }
        },
        {
            "name": "router",
            "workload": "router-perf-v2",
            "command": "./ingress-performance.sh",
            "env": {
                "THROUGHPUT_TOLERANCE": "10",
                "LATENCY_TOLERANCE": "10",
                "LARGE_SCALE_THRESHOLD": "19",
                "ENGINE": "local",
                "LARGE_SCALE_ROUTES": "500",
                "COMPARE": "false",
                "EMAIL_ID_FOR_RESULTS_SHEET": "msheth@redhat.com",
                "GSHEET_KEY_LOCATION": "/tmp/key.json"
            }
        },
        {
            "name": "load-cluster-preupgrade",
            "workload": "kube-burner",
            "command": "./run.sh",
            "env": {
                "WORKLOAD": "cluster-density",
                "JOB_ITERATIONS": "500",
                "JOB_TIMEOUT": "18000",
                "STEP_SIZE": "30s",
                "QPS": "20",
                "BURST": "20",
                "METRICS_PROFILE": "metrics-profiles/acs-metrics.yaml",
                "LOG_LEVEL": "info",
                "CLEANUP_WHEN_FINISH": "false",
                "CLEANUP": "true",
                "NODE_SELECTOR": "{node-role.kubernetes.io/workload: }"
            }
        },
        {
            "name": "upgrades",
            "workload": "upgrade-perf",
            "command": "./run_upgrade_fromgit.sh",
            "env": {
                "LATEST": "true",
                "CHANNEL": "nightlies",
                "TIMEOUT": "400",
                "POLL_INTERVAL": "10"
            }
        }
   ]
}
